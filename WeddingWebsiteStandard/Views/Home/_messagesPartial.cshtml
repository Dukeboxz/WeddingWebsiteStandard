@model IEnumerable<WeddingWebsiteStandard.Models.Message>

@{
    ViewBag.Title = "_messagesPartial";
}

<h2>_messagesPartial</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.messageText)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TimeOfMessage)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.messageText)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TimeOfMessage)
        </td>
       
    </tr>
}

</table>
